<h1>Bienvenidos a usuarios</h1>
<div>
    {{#each users}}
        <p><strong>Nombre: </strong>{{this.first_name}} - {{this.last_name}} - <strong>Email: </strong>{{this.email}}</p>
    {{/each}}
    <center>
        {{#if hasPrevPage}}
            <a href="/users?pageNum={{prevPage}}">Anterior</a>
        {{/if}}
        <label for="">{{page}}</label>
        {{#if hasNextPage}}
            <a href="/users?pageNum={{nextPage}}">Siguiente</a>
        {{/if}}
    </center>
</div>